<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FoodInEye_app" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black_4</item>
        <item name="colorPrimaryVariant">@color/carrot</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue</item>
        <item name="colorSecondaryVariant">@color/carrot</item>
        <item name="colorOnSecondary">@color/sygray</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->
        <item name="android:windowFullscreen">true</item>

        <!-- 폰트 적용 -->
        <item name="android:fontFamily">@font/mice_gothic_bold</item>
    </style>
</resources>